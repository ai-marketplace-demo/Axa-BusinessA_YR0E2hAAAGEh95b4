  "scripts": {
    "start": "set NODE_OPTIONS=--max_old_space_size=4096 && cross-env GENERATE_SOURCEMAP=true HOST='127.0.0.1' HTTPS=true  PORT=8080 REACT_APP_STAGE=dev react-scripts start",
    "__start": "GENERATE_SOURCEMAP=true PORT=1234 REACT_APP_STAGE=dev react-scripts start",
    "start:mushhz": "GENERATE_SOURCEMAP=true HTTPS=true PORT=1234 REACT_APP_STAGE=mushhz react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "build:staging": "GENERATE_SOURCEMAP=false REACT_APP_STAGE=staging npm run build",
    "build:mushhz": "GENERATE_SOURCEMAP=false REACT_APP_STAGE=mushhz npm run build",
    "deploy:staging": "aws s3 sync build/ s3://YOUR_DEV_S3_DEPLOY_BUCKET_NAME",
    "postdeploy:staging": "aws cloudfront create-invalidation --distribution-id YOUR_DEV_CF_DISTRIBUTION_ID --paths '/*' && aws cloudfront create-invalidation --distribution-id YOUR_DEV_WWW_CF_DISTRIBUTION_ID --paths '/*'",
    "predeploy:prod": "GENERATE_SOURCEMAP=false REACT_APP_STAGE=production npm run build",
    "deploy:prod": "aws s3 sync build/ s3://YOUR_PROD_S3_DEPLOY_BUCKET_NAME",
    "postdeploy:prod": "aws cloudfront create-invalidation --distribution-id YOUR_PROD_CF_DISTRIBUTION_ID --paths '/*' && aws cloudfront create-invalidation --distribution-id YOUR_PROD_WWW_CF_DISTRIBUTION_ID --paths '/*'",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
